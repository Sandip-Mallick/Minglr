{
    "cli": {
        "version": ">= 3.0.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://minglr-backend.onrender.com/api/v1"
            },
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://minglr-backend.onrender.com/api/v1"
            },
            "android": {
                "buildType": "apk"
            }
        }
    },
    "submit": {
        "production": {}
    }
}